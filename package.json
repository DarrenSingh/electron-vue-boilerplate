{
  "name": "vue-electron",
  "version": "0.1.0",
  "description": "application description",
  "author": {
    "name": "Your Name",
    "email": "youremail@mail.com"
  },
  "private": true,
  "main": "src/main/electron.js",
  "scripts": {
